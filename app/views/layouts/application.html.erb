<!DOCTYPE html>
<html>
<head>
  <title><%= title %></title>
  <%= stylesheet_link_tag "blueprint/screen", :media => 'screen' %>
  <%= stylesheet_link_tag 'custom', :media => 'screen' %>
  <%= csrf_meta_tag %>
</head>
<body>
	<div class="container">
		<%= render 'layouts/header' %>
		<section class ="round">
			<% flash.each do |key, value| %>
			<div class ="flash <%= key %>"><%= value %></div>
			<% end %>
<%= yield %>
</section>
</div>
</body>
</html>
